{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"custom": true,
			"link": true,
			"palette": [
				{"name": "Theme Color 1", "slug": "theme-color-1", "color": "#56585d"},
				{"name": "Theme Color 2", "slug": "theme-color-2", "color": "#d46a43"},
				{"name": "Theme Color 3", "slug": "theme-color-3", "color": "#6e6872"},
				{"name": "Theme Color 4", "slug": "theme-color-4", "color": "#5c4a28"},
				{"name": "Theme Color 5", "slug": "theme-color-5", "color": "#939393"},
				{"name": "Theme Color 6", "slug": "theme-color-6", "color": "#8a8a8a"},
				{"name": "Theme Color 7", "slug": "theme-color-7", "color": "#f0efee"},
				{"name": "Theme Color 8", "slug": "theme-color-8", "color": "#f8f8f8"},
				{"name": "White", "slug": "white", "color": "#FFFFFF"},
				{"name": "Black", "slug": "black", "color": "#000000"},
				{"name": "Soft White", "slug": "soft-white", "color": "#f5f5f5"},
				{"name": "Soft Black", "slug": "soft-black", "color": "#666666"},
				{"name": "Grey", "slug": "grey", "color": "#a5abab"}
			]
		},
		"custom": {
			"maxWidth": "1280px",
			"globalBorderRadius": "4px",
			"screens": {
				"small": "320px",
				"small-2": "600px",
				"small-3": "768px",
				"medium": "1024px",
				"large": "1280px",
				"xl": "1341px",
				"xxl": "1600px"
			},
			"typography": {
				"fontSize": {
					"body": "clamp( 14px, calc(1vw + 1rem), 17px )",
					"h1": "clamp( 36px, calc(3.5vw + 1rem), 60px )",
					"h2": "clamp( 24px, calc(2.8vw + 1rem), 48px )",
					"h3": "clamp( 20px, calc(2vw + 1rem), 38px )",
					"h4": "clamp( 18px, calc(1.75vw + 1rem), 30px )",
					"h5": "clamp( 17px, calc(1.25vw + 1rem), 24px )",
					"h6": "clamp( 16px, calc(1vw + 1rem), 20px )",
					"html": "62.5%"
				},
				"fontWeight": {
					"body": "300",
					"heading": "300"
				},
				"lineHeight": {
					"body": "1.6",
					"heading": "1.5"
				}
			}
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Montserrat, sans-serif",
					"slug": "coaching-pro-heading-font",
					"name": "Montserrat"
				},
				{
					"fontFamily": "Caveat, cursive, sans-serif",
					"slug": "coaching-pro-sub-heading-font",
					"name": "Caveat"
				},
				{
					"fontFamily": "Montserrat, sans-serif",
					"slug": "coaching-pro-body-font",
					"name": "Montserrat"
				}
			],
			"fontSizes": [
				{
					"slug": "x-small",
					"size": "10px",
					"name": "Extra Small: "
				},
				{
					"slug": "small",
					"size": "12",
					"name": "Small: "
				},
				{
					"slug": "medium",
					"size": "16",
					"name": "Medium: "
				},
				{
					"slug": "large",
					"size": "22",
					"name": "Large: "
				},
				{
					"slug": "super",
					"size": "32",
					"name": "Super: "
				},
				{
					"slug": "heading-1",
					"size": "90",
					"name": "Heading 1: "
				},
				{
					"slug": "heading-2",
					"size": "70",
					"name": "Heading 2: "
				},
				{
					"slug": "sub-heading",
					"size": "36",
					"name": "Sub-heading: "
				}
			]
		}
	},
	"styles": {
		"color": {
			"text": "#8A8A8A"
		},
		"typography": {
			"fontSize": "30px",
			"lineHeight": "55px"
		}
	}
}
